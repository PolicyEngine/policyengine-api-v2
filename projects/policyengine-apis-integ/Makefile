integ-test:
	poetry install
	$(if $(ACCESS_TOKEN),FULL_INTEG_TEST_ACCESS_TOKEN='$(ACCESS_TOKEN)') \
	$(if $(ACCESS_TOKEN),SIMULATION_INTEG_TEST_ACCESS_TOKEN='$(ACCESS_TOKEN)') \
	$(if $(FULL_API_URL),FULL_INTEG_TEST_BASE_URL='$(FULL_API_URL)') \
	$(if $(SIMULATION_API_URL),SIMULATION_INTEG_TEST_BASE_URL='$(SIMULATION_API_URL)') \
	poetry run pytest
